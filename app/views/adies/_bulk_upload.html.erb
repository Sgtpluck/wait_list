<div class='uploads'>
  <h3>
    Or All At Once!
  </h3>
  <%= form_tag('/csv', multipart: true) do %>
    <div class = "field">
        <%= label_tag "Upload CSV of Adies:" %>
        <%= file_field_tag :adies %>
    </div>
    <div class='field'>
      <%= submit_tag "Submit" %>
    </div>
  <% end %>
</div>